// Media Placeholder
//
// SVG basic settings
//
// Default svg value for the logo:
// ```
// $mediaPlaceholder: '../../svg/logo.svg'
// ```
// Default svg style for the logo:
// ```
// $mediaPlaceholder-style: '
//     path
//         fill rgba(255, 255, 255, .5)
// '
// ```
//
// Styleguide: Settings.MediaPlaceholder
$mediaPlaceholder ?= '../../svg/logo.svg'
$mediaPlaceholder-style ?= '
    path
        fill rgba(255, 255, 255, .5)
' // @stylint ignore
